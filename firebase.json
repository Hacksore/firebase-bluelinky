{
  "functions": {
    "predeploy": [
      "npm --prefix \"$RESOURCE_DIR\" run lint"
    ],
    "source": "functions"
  },
  "rewrites": [
    {
      "source": "/start",
      "function": "app"
    },
    {
      "source": "/lock",
      "function": "app"
    },
    {
      "source": "/status",
      "function": "app"
    }
  ]
}
